{
  "$schema": "./node_modules/@dagonmetric/angular-build/src/schemas/schema.json#",
  "libs": [
    {
      "extends": "ngb:lib-default",
      "name": "ng-translit",
      "root": "modules/ng-translit",
      "outputPath": "dist/packages/ng-translit",
      "libraryName": "ngTranslit",
      "envOverrides": {
        "prod": {
          "copy": [
            "README.md",
            "../../LICENSE"
          ],
          "bundles": true,
          "packageJsonCopy": true
        }
      }
    },
    {
      "extends": "ngb:lib-default",
      "name": "ng-translit-http-loader",
      "root": "modules/ng-translit/http-loader",
      "outputPath": "dist/packages/ng-translit",
      "libraryName": "ngTransli.httpLoader",
      "externals": {
        "@dagonmetric/ng-translit": "ngTranslit"
      },
      "envOverrides": {
        "prod": {
          "copy": [
            {
              "from": "README.md",
              "to": "http-loader/README.md"
            }
          ],
          "bundles": true,
          "packageJsonCopy": true
        }
      }
    }
  ],
  "apps": []
}
